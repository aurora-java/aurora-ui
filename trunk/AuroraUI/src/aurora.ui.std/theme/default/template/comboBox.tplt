<DIV class="${wrapClass} item-tf" style="width:${width}px;height:${height}px;${style}" id="${id}">
	<DIV style="position:relative;padding-left:3px;padding-right:20px;height:100%">
		<DIV style="height:100%;overflow:hidden">
			<INPUT tabindex="${tabindex}" style="height:${height}px;line-height:${height}px;${fontstyle}" class="item-textField" atype="field.input" autocomplete="off" value="${value}"/>
		</DIV>
		<DIV class="item-trigger item-comboButton" atype="triggerfield.trigger" hideFocus></DIV>
	</DIV>
	<DIV class="item-popup item-shadow" atype="triggerfield.popup" style="width:170px;">
		<DIV class="item-popup-content item-comboBox-view"></DIV>
	</DIV>
</DIV>
<script>
    	new Aurora.ComboBox(${config});
		${binding}
</script>