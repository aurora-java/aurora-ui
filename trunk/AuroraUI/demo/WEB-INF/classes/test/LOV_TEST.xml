<?xml version="1.0" encoding="UTF-8"?>
<sql-service>
	<fields>
		<field name="empno" datatype="java.lang.Long" databaseType="BIGINT" prompt="员工工号" queryWidth="150" forQuery="true" fordisplay="true" displayWidth="100"/>
		<field name="ename" datatype="java.lang.String" databaseType="VARCHAR" prompt="员工名" fordisplay="true" displayWidth="100"/>
		<field name="CREATION_DATE" datatype="java.lang.String" databaseType="VARCHAR" prompt="工作时间" fordisplay="true" displayWidth="100"/>
	</fields>
	<query>
		SELECT E.EMPNO,
			   E.ENAME,
			   E.CREATION_DATE
		  FROM EMP E
		#WHERE_CLAUSE# ORDER BY EMPNO
	</query>
	<query-fields>
		<query-field name="empno" dataType="java.lang.Integer" queryExpression="empno = ${@empno}"/>
		<query-field name="ename" dataType="java.lang.String" queryExpression="ename = ${@ename}"/>
	</query-fields>
</sql-service>













