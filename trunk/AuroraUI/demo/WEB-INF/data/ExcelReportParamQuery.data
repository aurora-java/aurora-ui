<?xml version="1.0" encoding="UTF-8"?>
<root>
<query Sql="select utl_excel_report.decode_session_id( ${@SESSIONIDSTR} ) as session_id from dual" ElementName="SID" />
<query Sql="select p.*
from excel_report_param p
where p.session_id = (
  select s.session_id
  from excel_report_session s
  where s.encoded_session_id = ${@SESSIONIDSTR}
)"  Target="param-list" Dump="false" />
</root>