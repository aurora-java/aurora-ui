<?xml version="1.0" encoding="UTF-8"?>
<root>
	<update Sql="
	declare
	   v_count number := 0;
	begin
	   select count(*) into v_count
     from   excel_report_session s
     where  s.session_id = ${/session/@session_id};
     if v_count=0 then
       insert into excel_report_session(session_id,encoded_session_id)
       values (${/session/@session_id}, utl_excel_report.get_encoded_session_id(${/session/@session_id}));
     end if;
	end;
	"/>
	<query ElementName="EXCEL-REPORT-SESSION">
		select  s.encoded_session_id 
		from     excel_report_session s
		where  s.session_id = ${/session/@session_id} 
	</query>
</root>
