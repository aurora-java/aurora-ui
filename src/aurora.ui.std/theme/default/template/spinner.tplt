<DIV class="${wrapClass} item-tf" style="position:relative;width:${width}px;height:${height}px;${style}" id="${id}" >
	<DIV style="padding-right:23px;height:100%">
		<DIV style="height:100%;overflow:hidden">
			<INPUT tabindex="${tabindex}" type="${inputtype}" style="ime-mode:disabled;${fontstyle}" class="item-textField item-spinner" atype="field.input" ${readonly} autocomplete="off" value="${value}" />
		</DIV>
		<DIV class="item-trigger item-spinner-btn" atype="triggerfield.trigger" hideFocus>
			<span class="item-spinner-plus"><div></div></span>
			<span class="item-spinner-minus"><div></div></span>
		</DIV>
	</DIV>
</DIV>
<script>
	new Aurora.Spinner(${config});
	${binding}
</script>